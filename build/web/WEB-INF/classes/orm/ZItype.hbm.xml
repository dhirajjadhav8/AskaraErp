<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="orm.ZItype" table="Z_ITYPE" schema="ERP">
        <id name="id" type="int">
            <column name="ID" />
            <generator class="native" />
        </id>
        <property name="itypeid" type="int">
            <column name="ITYPEID" not-null="true" />
        </property>
        <property name="name" type="string">
            <column name="NAME" />
        </property>
        <property name="description" type="string">
            <column name="DESCRIPTION" />
        </property>
        <property name="icategoryid" type="int">
            <column name="ICATEGORYID" />
        </property>
        <property name="opcode" type="char">
            <column name="OPCODE" length="1" not-null="true" />
        </property>
        <property name="modifiedby" type="int">
            <column name="MODIFIEDBY" />
        </property>
        <property name="modifiedon" type="timestamp">
            <column name="MODIFIEDON" length="29" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
