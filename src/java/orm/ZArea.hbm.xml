<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="orm.ZArea" table="Z_AREA" schema="ERP">
        <id name="id" type="int">
            <column name="ID" />
            <generator class="native" />
        </id>
        <property name="areaid" type="int">
            <column name="AREAID" not-null="true" />
        </property>
        <property name="code" type="string">
            <column name="CODE" />
        </property>
        <property name="roomsid" type="int">
            <column name="ROOMSID" />
        </property>
        <property name="flooresid" type="int">
            <column name="FLOORESID" />
        </property>
        <property name="buildingsid" type="int">
            <column name="BUILDINGSID" />
        </property>
        <property name="areatypesid" type="int">
            <column name="AREATYPESID" />
        </property>
        <property name="opcode" type="char">
            <column name="OPCODE" length="1" not-null="true" />
        </property>
        <property name="modifiedby" type="int">
            <column name="MODIFIEDBY" />
        </property>
        <property name="modifiedon" type="timestamp">
            <column name="MODIFIEDON" length="29" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
