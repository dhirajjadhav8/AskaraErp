<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="orm.ZInwordse" table="Z_INWORDSE" schema="ERP">
        <id name="id" type="int">
            <column name="ID" />
            <generator class="native" />
        </id>
        <property name="inwordseid" type="int">
            <column name="INWORDSEID" not-null="true" />
        </property>
        <property name="inNo" type="string">
            <column name="IN_NO" />
        </property>
        <property name="inDate" type="date">
            <column name="IN_DATE" length="10" />
        </property>
        <property name="type" type="java.lang.Character">
            <column name="TYPE" length="1" />
        </property>
        <property name="deliverymode" type="java.lang.Character">
            <column name="DELIVERYMODE" length="1" />
        </property>
        <property name="comments" type="string">
            <column name="COMMENTS" />
        </property>
        <property name="ifrom" type="java.lang.Integer">
            <column name="IFROM" />
        </property>
        <property name="forwardedTo" type="java.lang.Integer">
            <column name="FORWARDED_TO" />
        </property>
        <property name="isreceived" type="java.lang.Boolean">
            <column name="ISRECEIVED" />
        </property>
        <property name="opcode" type="char">
            <column name="OPCODE" length="1" not-null="true" />
        </property>
        <property name="modifiedby" type="java.lang.Integer">
            <column name="MODIFIEDBY" />
        </property>
        <property name="modifiedon" type="timestamp">
            <column name="MODIFIEDON" length="29" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
